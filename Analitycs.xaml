<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MauiApp1.Analitycs"
             Title="Аналітика">
    <VerticalStackLayout>
        <HorizontalStackLayout Margin="50, 10, 50, 10">
            <Label 
                Text="Виберіть дати"
                VerticalOptions="Center" 
                HorizontalOptions="Center" />
            <DatePicker x:Name="datePicker1" Format="d"/>
            <DatePicker x:Name="datePicker2" Format="d"/>
            <Label 
                Text="Виберіть лікаря"
                VerticalOptions="Center" 
                HorizontalOptions="Center" />
            <Picker x:Name="DentistP"/>
        </HorizontalStackLayout>

        <Button Text="Назад" FontSize="22" BorderWidth="1" Clicked="Back"  Margin="50, 10, 50, 10"/>

        <HorizontalStackLayout x:Name="stack" Margin="50, 10, 50, 10">

        </HorizontalStackLayout>
    </VerticalStackLayout>
</ContentPage>